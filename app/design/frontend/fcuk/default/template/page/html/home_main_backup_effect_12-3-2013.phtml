"z-index":1000,      "box-shadow":"0px 0px 18px rgba(51,51,51,0.4)"      }).parent("div").css({      "z-index":1000      }).find("div").find("a.expand_link").delay(500).fadeIn(500);  },function(){        $(this).find("a.expand_link").clearQueue();         var dataWidth = $(this).attr("data-width");    var dataHeight = $(this).attr("data-height");    $(this).stop();    $(this).stop().animate({      "top":0,      "left":0,      "width":dataWidth,      "height":dataHeight      }).css({      "z-index":10,      "box-shadow":"0px 0px 0px rgba(51,51,51,0.4)"      }).parent("div").css({      "z-index":10      }).find("div").find("a.expand_link").hide();  });      $(".view-hover-link").hover(function(){    $(this).find(".home_link").addClass('home_link_hover');    },function(){    $(this).find(".home_link").removeClass('home_link_hover');      });      $(".view-scale-video, .view-scale-video img").click(function(){    //$("#video_container_2").slideDown(1000).find("#video_player").load("video.htm");    $("#video_container_2").slideDown(1000).find("#video_player").html(      '<div style="padding:0px 0 0 0; text-align:center; position:relative;"><iframe width="974" height="548" src="http://www.youtube.com/embed/jFGdFmcmccw?rel=0&wmode=opaque" frameborder="0" allowfullscreen></iframe></div>'    );        $("#video_panel").hide();        if($('html').hasClass('lt-ie8')){          }else{        $('html, body').animate({      scrollTop: 640    }, 1000);        }  });      $("#close_video").click(function(){    $("#video_container_2").slideUp(1000).find("#video_player").html("");    $('html, body').animate({      scrollTop: 0    }, 1000);    $("#video_panel").show();  });      $("#video_container_close").click(function(){    $("#video_container").html("").animate({      "top":0,      "left":0,      "width":482,      "height":236      }).fadeOut(100);    $("#video_container_close").hide();    });  });</script>