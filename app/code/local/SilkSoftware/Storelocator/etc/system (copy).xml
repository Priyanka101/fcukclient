<?xml version="1.0"?>
<config>
	  <tabs>
		<iksula translate="label" module="storelocator">
            <label>Iksula</label>
            <sort_order>0</sort_order>
        </iksula>
	  </tabs>
	  <sections>
		<store  translate="label" module="storelocator">                    
		<label>Store</label>
		<tab>iksula</tab>
		<frontend_type>text</frontend_type>
		<sort_order>0</sort_order>
		<show_in_default>1</show_in_default>
		<show_in_website>1</show_in_website>
		<show_in_store>1</show_in_store>            
					<groups>
				      <storeinfo translate="label"> 
					  <label>Store Info</label>
					  <frontend_type>text</frontend_type>
					  <sort_order>0</sort_order>
					  <show_in_default>1</show_in_default>
					  <show_in_website>1</show_in_website>
					  <show_in_store>1</show_in_store>
				       <fields>
                          <storecities translate="label">
                            <label>Store Cities</label>
                            <frontend_type>select</frontend_type>
                            <source_model>SilkSoftware_Storelocator_Model_Mycustomoptions</source_model>
                            <sort_order>0</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment>please select default city</comment>
                          </storecities>
					   </fields>
					   </storeinfo>
					</groups>
		</store>
	  </sections>
</config>
